<p id="notice"><%= notice %></p>

<h1>Feeds</h1>
<table>
  <thead>
    <tr>
      <th class="title">title</th>
      <th class="content">content</th>
      <th class= "list">Picture</th>
      <th class="user">user</th>
      <th class= "created_at">created_at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feeds.each do |feed| %>
      <tr>
        <td><%= feed.image %></td>
        <td class= "created_at"><%= l(feed.created_at, format: :short)%></td>
        <td><%= link_to 'Show', feed %></td>
        <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
        <td><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div class="link">
<%= link_to 'New Feed', new_feed_path %>
