<h1>confirmation screen</h1>
<%= form_with(model: @feed, local: true) do |form| %>
  <%= form.hidden_field :title %>

  <%= form.hidden_field :content %>

  <div class="user">
  </div>
  <div class="image">
    <p>image</p>
  </div>
  <%= form.hidden_field :image_cache %>
  <%= form.hidden_field :current_user %>
  <%= form.submit "submit" %>
<% end %>
<%= form_with(model:@feed, url:feeds_path, local:true, method:"get") do |form| %>
  <%= form.hidden_field :image_cache %>
  <%= form.submit "return", name: "back" %>
<% end %>
